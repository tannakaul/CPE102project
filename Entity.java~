class Entity
{
    private String name;
    private Point position; 
    Entity(String name, Point position)
    {
        this.name = name;
        this.position = position;
    }
    public Point set_position(Point point)
    {
        this.position = point;
	return position;
	}
    public Point get_position()
    {
        return this.position;
    }
    public String get_name()
    {
        return this.name;
    }
}